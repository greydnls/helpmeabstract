{
  "name": "kayladaniels/helpmeabstract",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/kayladnls/php-github-api"
    }
  ],
  "require": {
    "slim/slim": "^2.6.2",
    "vlucas/phpdotenv": "~1.1@dev",
    "twig/twig": "^1.34.4",
    "guzzlehttp/guzzle": "^6.0.0",
    "robmorgan/phinx": "dev-master",
    "vlucas/spot2": "^2.1.7",
    "mailgun/mailgun-php": "^2.0",
    "php-http/guzzle6-adapter": "^1.0",
    "doctrine/dbal": "2.4",
    "doctrine/annotations": "1.2.7"
  },
  "authors": [
    {
      "name": "Graham Daniels",
      "email": "greydnls@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": { "HelpMeAbstract\\": "src/" }
  },
  "minimum-stability": "stable",
  "scripts": {
    "post-install-cmd": [
        "@php -r \"file_exists('.env') || copy('.env.changeme', '.env');\""
    ]
  }
}
